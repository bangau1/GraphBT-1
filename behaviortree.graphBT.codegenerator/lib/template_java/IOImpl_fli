package [P];

// included in the absfrontend.jar
import abs.backend.java.lib.types.ABSString;
import abs.backend.java.lib.types.ABSInteger;
import java.util.*;
// generated by the ABS Java backend
import [P].IOImpl_c;

public class IOImpl_fli extends IOImpl_c {
	Scanner in = new Scanner(System.in);
	public ABSString fli_print(ABSString msg) {
		System.out.println(msg.getString());
		return ABSString.fromString("Hello ABS, this is Java");
	}
	public ABSInteger fli_readInt() {
		return ABSInteger.fromInt(in.nextInt());
	}
	public ABSString fli_readString() {
		return ABSString.fromString(in.next());
	}
}
